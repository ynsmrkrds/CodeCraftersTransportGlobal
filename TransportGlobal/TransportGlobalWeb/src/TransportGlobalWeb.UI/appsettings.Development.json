{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApiEndpoints": {
    "BaseUrl": "https://localhost:7091/",
    "PageSize": 10,
    "UserContextEndpoints": {
      "UserEndpoints": {
        "Login": "api/User/login",
        "Register": "api/User/register",
        "GetProfile": "api/User/profile",
        "UpdateProfile": "api/User/updateProfile",
        "UpdatePassword": "api/User/updatePassword"
      }
    },
    "TransporterContextEndpoints": {
      "CompanyEndpoints": {
        "CrudCompany": "api/Company",
        "GetOwnCompany": "api/Company/own"
      },
      "VehicleEndpoints": {
        "CrudVehicle": "api/Vehicle",
        "GetOwnVehicles": "api/Vehicle/own"
      },
      "EmployeeEndpoints": {
        "CrudEmployee": "api/Employee",
        "GetOwnEmployees": "api/Employee/own",
        "GetVehicleEmployees": "api/Employee/vehicle"
      }
    },
    "TransportContextEndpoints": {
      "TransportRequestEndpoints": {
        "CrudTransportRequest": "api/TransportRequest",
        "GetOwnTransportRequest": "api/TransportRequest/own",
        "GetPendingTransportRequests": "api/TransportRequest/pendings"
      },
      "TransportContractEndpoints": {
        "GetOwnTransportContracts": "api/TransportContract/own",
        "CrudTransportContract": "api/TransportContract",
        "AgreeTransportContract": "api/TransportContract/agree"
      }
    }
  }
}
